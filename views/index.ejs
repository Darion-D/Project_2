<!DOCTYPE html>
<html lang="en">
<link rel="stylesheet" href="/styles/index.css" />

<%- include( 'partials/head.ejs' , { page: 'Products' } ) %>

    <body>
        <%- include( 'partials/nav.ejs' ) %>

            <section>
                <div>
                    <h2 class="title">All Videos</h2>
                </div>
            </section>
            <div class="videos">
                <% for( let i=0; i<videos.length; i++){ %>
                    <% const firstVideo=videos[i] %>
                        <a id="video" href="/videos/<%= firstVideo._id %>">
                            <%= firstVideo.title %>
                        </a>
                        <% } %>
            </div>
    </body>

</html>